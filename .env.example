# 向量数据库类型，固定为 chroma-cloud
VECTOR_STORE=chroma-cloud

# Chroma Cloud 相关配置
CHROMA_TENANT=
CHROMA_API_KEY=
CHROMA_DATABASE=
CHROMA_COLLECTION=

# GitHub OAuth 凭证（替换成你自己的）
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# 应用访问地址（如：https://gimme-icon-next.vercel.app）
NEXTAUTH_URL=

# 生成地址：https://generate-secret.vercel.app/32
# 加密会话的密钥（必须有，用于加密Cookie/Session，随便生成一个长字符串即可）
BETTER_AUTH_SECRET=xxx

# 允许访问 /console 的账号邮箱，逗号分隔
CONSOLE_ALLOWED_EMAILS=your_email@example.com,admin@example.com
